{
  "name": "Devresources",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "3.26.9",
    "aws-amplify": "1.1.29",
    "lodash": "4.17.13",
    "react": "16.8.6",
    "react-device-detect": "1.6.2",
    "react-dom": "16.8.6",
    "react-graceful-image": "1.2.9",
    "react-responsive": "6.1.2",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.8",
    "url-query-search": "0.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "unit": "react-scripts test",
    "eject": "react-scripts eject",
    "pre-deploy": "gh-pages -d build",
    "deploy": "aws s3 cp ./build s3://devresources.site/ --recursive",
    "invalidation": "aws cloudfront create-invalidation --distribution-id $DISTRIBUTION_ID --paths '/*'",
    "github": "git push github master",
    "pre-commit": "lint-staged",
    "eslint": "eslint src/*.js --fix",
    "cypress": "./node_modules/.bin/cypress open",
    "test": "./node_modules/.bin/cypress run",
    "report": "node ./cypress/utils/runAndReport.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "lint-staged": {
    "src /**/*.{js,jsx}": [
      "eslint --fix",
      "pretty-quick â€” staged",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "cypress": "3.3.1",
    "cypress-multi-reporters": "1.1.22",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-react": "7.13.0",
    "fs-extra": "8.0.1",
    "gh-pages": "2.0.1",
    "husky": "2.4.1",
    "lint-staged": "8.2.1",
    "mocha": "6.1.4",
    "mocha-junit-reporters": "1.23.6",
    "mochawesome": "4.0.1",
    "mochawesome-merge": "2.0.1",
    "mochawesome-report-generator": "4.0.0",
    "pm2": "3.5.1",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11"
  ]
}
